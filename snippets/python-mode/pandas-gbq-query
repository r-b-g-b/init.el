import random
import string

# Generate a two-character suffix
suffix = ''.join(random.choices(string.ascii_lowercase + string.digits, k=2))

df${suffix} = pandas_gbq.read_gbq(
    """
    select
        ${1:<select>}
    from
        ${2:<table>}
    """
)
